<Device>
  <Name>Arturia Keylab Mkii 61</Name>
  <Author>Aisjam</Author>
  <Description>Controlmap for Arturia Keylab Mkii 61. set to DAW > Standard MCU</Description>
  <States>

  </States>
  
  <Parameters>
    <Row>      
      <!-- Pressuresensitive grid -->
      <Column>
        <Row>
          <Group name="Pads" orientation="horizontal" columns="4">
          <!--
            <Param value="C-3" size="1.5" type="togglebutton" colorspace = "8,8,8" name="pad 01" maximum="127" minimum="0"/>
            <Param value="C#3" size="1.5" type="togglebutton" colorspace = "8,8,8" name="pad 02" maximum="127" minimum="0"/>
            <Param value="D-3" size="1.5" type="togglebutton" colorspace = "8,8,8" name="pad 03" maximum="127" minimum="0"/>
            <Param value="D#3" size="1.5" type="togglebutton" colorspace = "8,8,8" name="pad 04" maximum="127" minimum="0"/>
            <Param value="G#2" size="1.5" type="togglebutton" colorspace = "8,8,8" name="pad 05" maximum="127" minimum="0"/>
            <Param value="A-2" size="1.5" type="togglebutton" colorspace = "8,8,8" name="pad 06" maximum="127" minimum="0"/>
            <Param value="A#2" size="1.5" type="togglebutton" colorspace = "8,8,8" name="pad 07" maximum="127" minimum="0"/>
            <Param value="B-2" size="1.5" type="togglebutton" colorspace = "8,8,8" name="pad 08" maximum="127" minimum="0"/>
            <Param value="E-2" size="1.5" type="togglebutton" colorspace = "8,8,8" name="pad 09" maximum="127" minimum="0"/>
            <Param value="F-2" size="1.5" type="togglebutton" colorspace = "8,8,8" name="pad 10" maximum="127" minimum="0"/>
            <Param value="F#2" size="1.5" type="togglebutton" colorspace = "8,8,8" name="pad 11" maximum="127" minimum="0"/>
            <Param value="G-2" size="1.5" type="togglebutton" colorspace = "8,8,8" name="pad 12" maximum="127" minimum="0"/>
            <Param value="C-2" size="1.5" type="togglebutton" colorspace = "8,8,8" name="pad 13" maximum="127" minimum="0"/>
            <Param value="C#2" size="1.5" type="togglebutton" colorspace = "8,8,8" name="pad 14" maximum="127" minimum="0"/>
            <Param value="D-2" size="1.5" type="togglebutton" colorspace = "8,8,8" name="pad 15" maximum="127" minimum="0"/>
            <Param value="D#2" size="1.5" type="togglebutton" colorspace = "8,8,8" name="pad 16" maximum="127" minimum="0"/>
            -->
          </Group>
        </Row>
      </Column>
         
      <!-- DAW Commands/ Transprts -->   
      <Column>
        <Row>
          <Group name="DAWCMDSwitches" orientation="horizontal" columns="5">
            <Param value="B--1" aspect="0.6" type="button" colorspace = "1,1,1" name="Solo" maximum="127" minimum="0"/>
            <Param value="G-0" aspect="0.6" type="button" colorspace = "1,1,1" name="Mute" maximum="127" minimum="0"/>
            <Param value="D#-1" aspect="0.6" type="button" colorspace = "1,1,1" name="Record" maximum="127" minimum="0"/>
            <Param value="D-5" aspect="0.6" type="button" colorspace = "1,1,1" name="Read" maximum="127" minimum="0"/>
            <Param value="D#5" aspect="0.6" type="button" colorspace = "1,1,1" name="Write" maximum="127" minimum="0"/>
            <Param value="G#5" aspect="0.6" type="button" colorspace = "1,1,1" name="Save" maximum="127" minimum="0"/>
            <Param value="D#6" aspect="0.6" type="button" colorspace = "1,1,1" name="In" maximum="127" minimum="0"/>
            <Param value="E-6" aspect="0.6" type="button" colorspace = "1,1,1" name="Out" maximum="127" minimum="0"/>
            <Param value="F-6" aspect="0.6" type="button" colorspace = "1,1,1" name="Metro" maximum="127" minimum="0"/>
            <Param value="A-5" aspect="0.6" type="button" colorspace = "1,1,1" name="Undo" maximum="127" minimum="0"/>
          </Group>
        </Row>
        
        <Row>
          <Group name="PlaySwitches" orientation="horizontal" columns="6">
            <Param value="G-6" size="0.833" type="button" colorspace = "1,1,1" name="reverse" maximum="127" minimum="0"/>
            <Param value="G#6" size="0.833" type="button" colorspace = "1,1,1" name="forward" maximum="127" minimum="0"/>
            <Param value="A-6" size="0.833" type="button" colorspace = "1,1,1" name="Stop" maximum="127" minimum="0"/>
            <Param value="A#6" size="0.833" type="button" colorspace = "1,1,1" name="play" maximum="127" minimum="0"/>
            <Param value="B-6" size="0.833" type="button" colorspace = "1,1,1" name="record" maximum="127" minimum="0"/>
            <Param value="D-6" size="0.833" type="button" colorspace = "1,1,1" name="loop" maximum="127" minimum="0"/>
          </Group>
        </Row>
      </Column>
      
      <!-- Center-->   
      <Column>
          <!-- if you want big dial as a potentiometer-->
        <Group name="CenterConsoleDial" orientation="horizontal" columns="3">
          <Param value="CC#60" size="3" type="dial" mode="rel_7_signed2" name="Big Encoder" maximum="127" minimum="0"/>
        </Group>
        <!-- if you want big dial as 2 buttons -->
        <Row>
          <Group name="CenterConsoleDial_LeftBtn" orientation="horizontal" columns="2">
            <Param value="CC#60" type="button" size="1.4" aspect="0.3" name="left_Rot" maximum="65" minimum="0"/>
          </Group>
          <Group name="CenterConsoleDial_RightBtn" orientation="horizontal" columns="2">
            <Param value="CC#60" type="button" size="1.4" aspect="0.3" name="Right_Rot"maximum="1" minimum="0"/>
          </Group>
        </Row>
        <Row>
          <Group name="CenterConsole" orientation="horizontal" columns="3">
            <Param value="D-7" type="button" colorspace = "1,1,1" name="left" maximum="127" minimum="0"/>
            <Param value="C-6" type="button" colorspace = "1,1,1" name="Big Encoder Btn" maximum="127" minimum="0"/>
            <Param value="D#7" type="button" colorspace = "1,1,1" name="Right" maximum="127" minimum="0"/>
          </Group>
        </Row>
      </Column>
      
      <!-- Faders Btns --> 
      <Column>
        <Row>
          <Group name="Encoders" orientation="horizontal" columns="8">
            <Param value="CC#16" type="dial" mode="rel_7_signed2" name="Encoder #1" maximum="127" minimum="0"/>
            <Param value="CC#17" type="dial" mode="rel_7_signed2" name="Encoder #2" maximum="127" minimum="0"/>
            <Param value="CC#18" type="dial" mode="rel_7_signed2" name="Encoder #3" maximum="127" minimum="0"/>
            <Param value="CC#19" type="dial" mode="rel_7_signed2" name="Encoder #4" maximum="127" minimum="0"/>
            <Param value="CC#20" type="dial" mode="rel_7_signed2" name="Encoder #5" maximum="127" minimum="0"/>
            <Param value="CC#21" type="dial" mode="rel_7_signed2" name="Encoder #6" maximum="127" minimum="0"/>
            <Param value="CC#22" type="dial" mode="rel_7_signed2" name="Encoder #7" maximum="127" minimum="0"/>
            <Param value="CC#23" type="dial" mode="rel_7_signed2" name="Encoder #8" maximum="127" minimum="0"/>
          </Group>
        </Row>
        
        <Row>
          <Group name="Faders" orientation="horizontal" columns="8">
            <Param value="PB|Ch1" type="fader" size="3" orientation="vertical" name="Fader #1" maximum="127" minimum="0"/>
            <Param value="PB|Ch2" type="fader" size="3" orientation="vertical" name="Fader #2" maximum="127" minimum="0"/>
            <Param value="PB|Ch3" type="fader" size="3" orientation="vertical" name="Fader #3" maximum="127" minimum="0"/>
            <Param value="PB|Ch4" type="fader" size="3" orientation="vertical" name="Fader #4" maximum="127" minimum="0"/>
            <Param value="PB|Ch5" type="fader" size="3" orientation="vertical" name="Fader #5" maximum="127" minimum="0"/>
            <Param value="PB|Ch6" type="fader" size="3" orientation="vertical" name="Fader #6" maximum="127" minimum="0"/>
            <Param value="PB|Ch7" type="fader" size="3" orientation="vertical" name="Fader #7" maximum="127" minimum="0"/>
            <Param value="PB|Ch8" type="fader" size="3" orientation="vertical" name="Fader #8" maximum="127" minimum="0"/>
          </Group>
        </Row>
        
        <Row>
          <Group name="Switches_mute" orientation="horizontal" columns="8" colorspace = "1,1,1">
            <Param value="C-1" aspect="0.6" type="pushbutton" colorspace = "8,8,8" name="Switch 1" maximum="127" minimum="0"/>
            <Param value="C#1" aspect="0.6" type="pushbutton" colorspace = "8,8,8" name="Switch 2" maximum="127" minimum="0"/>
            <Param value="D-1" aspect="0.6" type="pushbutton" colorspace = "8,8,8" name="Switch 3" maximum="127" minimum="0"/>
            <Param value="D#1" aspect="0.6" type="pushbutton" colorspace = "8,8,8" name="Switch 4" maximum="127" minimum="0"/>
            <Param value="E-1" aspect="0.6" type="pushbutton" colorspace = "8,8,8" name="Switch 5" maximum="127" minimum="0"/>
            <Param value="F-1" aspect="0.6" type="pushbutton" colorspace = "8,8,8" name="Switch 6" maximum="127" minimum="0"/>
            <Param value="F#1" aspect="0.6" type="pushbutton" colorspace = "8,8,8" name="Switch 7" maximum="127" minimum="0"/>
            <Param value="G-1" aspect="0.6" type="pushbutton" colorspace = "8,8,8" name="Switch 8" maximum="127" minimum="0"/>
          </Group>
        </Row>
        
      </Column>
      
      <!-- master -->
       <Column>
        <Row>
          <Group name="MasterEncoder" orientation="horizontal" columns="9">
            <Param value="CC#24" type="dial" mode="rel_7_signed2" name="Encoder #9" maximum="127" minimum="0"/>
          </Group>
        </Row>
        
        <Row>
          <Group name="MasterFader" orientation="horizontal" columns="9">
            <Param value="PB|Ch9" type="fader" size="3" orientation="vertical" name="Master" maximum="127" minimum="0"/>
          </Group>
        </Row>
        
        <Row>
          <Group name="MasterSwitch" orientation="horizontal" columns="9">
            <Param value="CC#109" aspect="0.6" type="button" name="Switch 8" maximum="127" minimum="0"/>
          </Group>
        </Row>
      </Column>
    
    </Row>
  </Parameters>
</Device>
